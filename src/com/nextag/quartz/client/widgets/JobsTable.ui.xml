<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="../common.css">
		 .displayInline {
      	display: inline;
      }
      
      .table {
      	width: 100%;
      	height: 100%;
      	table-layout: fixed;
      }
	</ui:style>

	<g:DockLayoutPanel unit="EX">
		<g:north size="5.7">
			<g:DockLayoutPanel unit="EX" width="" height="20">
				<g:east size="30.0">
					<g:SimplePanel>
						<p1:SimplePager location="CENTER" stylePrimaryName="{style.rightAlign}" display="{table}" />
					</g:SimplePanel>
				</g:east>
			</g:DockLayoutPanel>
		</g:north>
		<g:center>
			<p1:DataGrid ui:field="table" stylePrimaryName="{style.table}"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
